{
  "name": "electrode-keepalive",
  "version": "0.3.10",
  "description": "electrode-keepalive",
  "main": "lib/electrode-keepalive.js",
  "scripts": {
    "test": "clap electrode/test",
    "coverage": "clap electrode/check",
    "prepublishOnly": "npm run coverage && publish-util-prepublishonly",
    "postpack": "publish-util-postpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/electrode-keepalive.git"
  },
  "keywords": [
    "electrode",
    "hapi",
    "node",
    "webserver"
  ],
  "files": [
    "agentkeepalive",
    "lib"
  ],
  "contributors": [
    {
      "name": "Jason Wilson",
      "email": "jrwilson@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 4.2.2"
  },
  "dependencies": {
    "agentkeepalive": "4.1.4",
    "humanize-ms": "^1.2.1"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "electrode-archetype-njs-module-dev": "^2.2.0",
    "publish-util": "^1.1.6",
    "sinon": "^1.17.6",
    "superagent": "^2.1.0"
  },
  "xclap": {
    "npm": true
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "coverage",
      "*clap.js",
      "gulpfile.js",
      "dist",
      "test",
      "agentkeepalive"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": true
  }
}
